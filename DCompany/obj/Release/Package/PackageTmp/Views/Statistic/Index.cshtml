@{
    ViewBag.Title = "Index";
    var list = (List<String>)ViewBag.ListYear;
    
}

<div class="container">
        <ul class="breadcrumb ">
    <li class="active">
      <h4> Thống kê năm </h4>        
       <select id="year" onchange="changeYear()">
      <option value="2013">2013</option>
      <option selected value="2014">2014</option>
      <option value="2015">2015</option>
      <option value="2016">2016</option>
      <option value="2017">2017</option>
      <option value="2018">2018</option>
      <option value="2019">2019</option>
      <option value="2020">2020</option>
</select>
    </li>
  </ul>
</div>
<div id="listYear"></div>
<script>
    $(document).ready(changeYear());
    function changeYear() {
        var year = $('#year').val();
        $.ajax({
            url: "/Statistic/ChangYear",
            type: "GET",
            data: { YEAR: year },
            success: function (data) {
                $('#listYear').html(data)
            }
        }
                        );
    }
</script>
<script src="~/Js/highcharts.js"></script>